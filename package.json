{
  "name": "nextjs-clean-boilerplate",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "packageManager": "bun@1.2.19",
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:all": "mprocs ",
    "build": "next build --webpack",
    "start": "next start --port 4000",
    "lint": "next lint --fix",
    "test": "vitest",
    "inngest-dev": "npx --ignore-scripts=false inngest-cli@latest dev",
    "seed": "bun --env-file=.env.local ./src/bootstrap/boundaries/db/seed.ts",
    "db:setup": "bunx drizzle-kit migrate && bun run seed",
    "storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "test:run": "vitest run && playwright test",
    "test:unit": "vitest run",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "prepare": "husky install",
    "analyze": "bun run build && bunx vite-bundle-visualizer",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "db:migrate:run": "bun --env-file=.env.local ./src/bootstrap/boundaries/db/migrate.ts",
    "db:reset": "bun --env-file=.env.local ./src/bootstrap/boundaries/db/reset-and-migrate.ts",
    "db:verify": "bun --env-file=.env.local ./src/bootstrap/boundaries/db/verify-db.ts",
    "db:studio": "bunx drizzle-kit studio"
  },
